{
  "agents": {
    "main": {
      "name": "Claude Sonnet 4.5",
      "provider": "anthropic",
      "model": "claude-sonnet-4-5-20250929",
      "temperature": 0.7,
      "maxTokens": 8000,
      "apiKeyEnv": "ANTHROPIC_API_KEY",
      "role": "primary",
      "capabilities": [
        "code-generation",
        "analysis",
        "refactoring",
        "documentation",
        "architecture"
      ]
    },
    "fallback": {
      "name": "GPT-4 Turbo",
      "provider": "openai",
      "model": "gpt-4-turbo-preview",
      "temperature": 0.7,
      "maxTokens": 4000,
      "apiKeyEnv": "OPENAI_API_KEY",
      "role": "fallback",
      "capabilities": [
        "code-generation",
        "analysis",
        "quick-tasks"
      ]
    },
    "research": {
      "name": "Perplexity Research",
      "provider": "perplexity",
      "model": "llama-3.1-sonar-large-128k-online",
      "temperature": 0.5,
      "maxTokens": 4000,
      "apiKeyEnv": "PERPLEXITY_API_KEY",
      "role": "research",
      "capabilities": [
        "web-search",
        "research",
        "documentation-lookup",
        "api-discovery"
      ]
    }
  },
  "project": {
    "name": "Pikl QA Assistant",
    "description": "AI-powered call center quality assurance automation",
    "rootDir": "/Users/iamjohndass/Sites/pikl-qa-assist",
    "outputDir": "taskmaster/output",
    "envFile": ".env.local"
  },
  "taskManagement": {
    "maxContextTokens": 100000,
    "chunkSize": 50000,
    "enableRAG": true,
    "vectorStore": "local",
    "summaryGeneration": true
  },
  "workflow": {
    "defaultAgent": "main",
    "autoFallback": true,
    "researchFirst": true,
    "validateOutputs": true
  }
}
